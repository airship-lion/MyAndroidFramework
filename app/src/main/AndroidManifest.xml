<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="study.toonan.com.myframework">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        tools:replace="android:icon"
        android:theme="@style/AppTheme">
        <meta-data android:name="design_width" android:value="1080">
        </meta-data>
        <meta-data android:name="design_height" android:value="1920">
        </meta-data>
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RentPatrolListActivity"/>
        <activity android:name=".activity.RentPatrolListActivity2"/>
        <activity android:name=".activity.baseAdapterActivity"/>
        <activity android:name=".activity.baseAdapterActivity2"/>
        <activity android:name=".activity.baseAdapterActivity3"/>
        <activity android:name=".activity.ViewPagerActivity"/>
    </application>

</manifest>